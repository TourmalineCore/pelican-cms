{
  "kind": "collectionType",
  "collectionName": "documents",
  "info": {
    "singularName": "document",
    "pluralName": "documents",
    "displayName": "Документы",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "showDate": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "files": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "files"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::documents-category.documents-category",
      "required": true
    },
    "subtitle": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "date": {
      "type": "date",
      "required": true
    }
  }
}
